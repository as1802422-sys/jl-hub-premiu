-- [JL HUB PRINCIPAL]
local plr = game:GetService("Players").LocalPlayer
local mouse = plr:GetMouse()
local cg2 = game:GetService("CoreGui")
if cg2:FindFirstChild("JLHubMain") then cg2.JLHubMain:Destroy() end

local gui2 = Instance.new("ScreenGui", cg2)
gui2.Name = "JLHubMain"
gui2.ResetOnSpawn = false

local bg2 = Instance.new("ImageLabel", gui2)
bg2.Image = "rbxassetid://14924794312"
bg2.Size = UDim2.new(1,0,1,0)
bg2.Position = UDim2.new(0,0,0,0)
bg2.ImageTransparency = 0.8
bg2.BackgroundTransparency = 1

local frame = Instance.new("Frame", gui2)
frame.Size = UDim2.new(0,272,0,468)
frame.Position = UDim2.new(0,14,0.19,0)
frame.BackgroundColor3 = Color3.fromRGB(27,30,31)
frame.BackgroundTransparency = 0.13
frame.BorderSizePixel = 0
Instance.new("UICorner", frame).CornerRadius = UDim.new(0,13)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1,0,0,43)
title.BackgroundTransparency = 1
title.Text = "ðŸŒ³ JL Hub - Blox Fruits Ultimate"
title.Font = Enum.Font.GothamBold
title.TextSize = 20
title.TextColor3 = Color3.fromRGB(110,255,148)
title.TextStrokeTransparency = 0.5

local states, threads = {}, {}

local function notify(msg)
    pcall(function() game.StarterGui:SetCore("SendNotification", {Title = "JL Hub", Text = msg, Duration=2.2}) end)
end

local function makeBtn(txt, y, color, fun)
    local b = Instance.new("TextButton", frame)
    b.Text = txt
    b.Font = Enum.Font.GothamMedium
    b.Size = UDim2.new(1,-18,0,34)
    b.Position = UDim2.new(0,9,0,44+y)
    b.BackgroundColor3 = color
    b.TextColor3 = Color3.fromRGB(217,255,209)
    b.TextSize = 17
    b.BorderSizePixel = 0
    Instance.new("UICorner", b).CornerRadius=UDim.new(0,8)
    b.MouseButton1Click:Connect(function() fun(b) end)
    return b
end

-- Util mob da quest
local function getQuestMob()
    local quest = plr.PlayerGui and plr.PlayerGui:FindFirstChild("Main") and plr.PlayerGui.Main:FindFirstChild("Quest")
    if quest and quest.Title and quest.Title.Text and #quest.Title.Text > 0 then
        return quest.Title.Text:match("%[(.-)%]")
    end
    return nil
end

-- [1] Auto Farm Mob (quest)
states.farm = false
makeBtn("ðŸŒ¾ Auto Farm Mob (Quest)",0,Color3.fromRGB(84,156,239),function(btn)
    states.farm = not states.farm
    btn.BackgroundColor3 = states.farm and Color3.fromRGB(18,180,80) or Color3.fromRGB(84,156,239)
    if states.farm then
        notify("Auto Farm Mob ON")
        if threads.farm then threads.farm:Disconnect() end
        threads.farm = game:GetService("RunService").Heartbeat:Connect(function()
            if not states.farm then return end
            local mobName = getQuestMob()
            if not mobName then return end
            for _,npc in ipairs(workspace.Enemies:GetChildren()) do
                if npc:IsA("Model") and npc:FindFirstChild("Humanoid") and npc.Humanoid.Health>0 and npc.Name:find(mobName) then
                    local char = plr.Character
                    if char and char:FindFirstChild("HumanoidRootPart") and npc:FindFirstChild("HumanoidRootPart") then
                        char.HumanoidRootPart.CFrame = npc.HumanoidRootPart.CFrame + Vector3.new(0,3,0)
                    end
                end
            end
        end)
    else
        notify("Auto Farm Mob OFF")
        if threads.farm then threads.farm:Disconnect(); threads.farm=nil end
    end
end)

-- [2] Auto Boss
states.boss = false
makeBtn("ðŸ‘‘ Auto Boss",38,Color3.fromRGB(255,162,62),function(btn)
    states.boss = not states.boss
    btn.BackgroundColor3 = states.boss and Color3.fromRGB(232,184,47) or Color3.fromRGB(255,162,62)
    local bossNames = {
        "Bobby","Yeti","Saber Expert","Magma Admiral","Fishman Lord","Warden","Chief Warden","Swan",
        "Vice Admiral","Don Swan","Fujita","Cyborg","Diamond","Jeremy","Fajita","Smoke Admiral",
        "Awakened Ice Admiral","Order","Flamingo","Darkbeard","Greybeard","Kilo Admiral",
        "Island Empress","Tide Keeper","Longma","Stone","Beautiful Pirate","rip_indra","Cake Queen","Cursed Captain","Mammoth","Captain Elephant"
    }
    if states.boss then
        notify("Auto Boss ON")
        if threads.boss then threads.boss:Disconnect() end
        threads.boss = game:GetService("RunService").Heartbeat:Connect(function()
            if not states.boss then return end
            for _,npc in ipairs(workspace.Enemies:GetChildren()) do
                if npc:IsA("Model") and npc:FindFirstChild("Humanoid") and npc.Humanoid.Health>0 then
                    for _,boss in ipairs(bossNames) do
                        if npc.Name:lower():find(boss:lower()) then
                            local char = plr.Character
                            if char and char:FindFirstChild("HumanoidRootPart") and npc:FindFirstChild("HumanoidRootPart") then
                                char.HumanoidRootPart.CFrame = npc.HumanoidRootPart.CFrame + Vector3.new(0,7,0)
                            end
                        end
                    end
                end
            end
        end)
    else
        notify("Auto Boss OFF")
        if threads.boss then threads.boss:Disconnect(); threads.boss=nil end
    end
end)

-- [3] AutoClick
states.click = false
makeBtn("âš¡ Auto Click",76,Color3.fromRGB(92, 45, 140),function(btn)
    states.click = not states.click
    btn.BackgroundColor3 = states.click and Color3.fromRGB(110,38,197) or Color3.fromRGB(92,45,140)
    if states.click then
        notify("Auto Click ON")
        if threads.click then threads.click:Disconnect() end
        threads.click = game:GetService("RunService").Heartbeat:Connect(function()
            if not states.click then return end
            local char=plr.Character
            if char and char:FindFirstChildWhichIsA("Tool") then
                for _,ev in pairs(getconnections(mouse.Button1Down)) do ev:Fire() end
            end
        end)
    else
        notify("Auto Click OFF")
        if threads.click then threads.click:Disconnect(); threads.click=nil end
    end
end)

-- [4] ESP players
states.esp = false; local espParts = {}
makeBtn("ðŸ”´ ESP Vermelho",118,Color3.fromRGB(190,17,30),function(btn)
    states.esp = not states.esp
    btn.BackgroundColor3 = states.esp and Color3.fromRGB(167,40,22) or Color3.fromRGB(190,17,30)
    if states.esp then
        notify("ESP ON")
        for _,p in ipairs(game:GetService("Players"):GetPlayers()) do
            if p~=plr and p.Character and p.Character:FindFirstChild("Head") then
                local a=Instance.new("BoxHandleAdornment")
                a.Adornee=p.Character.Head;a.Size=Vector3.new(1.9,1.9,1.9)
                a.Color3=Color3.new(1,0,0);a.Transparency=.6;a.AlwaysOnTop=true;a.ZIndex=12
                a.Parent=cg2
                table.insert(espParts,a)
            end
        end
    else
        notify("ESP OFF")
        for _,a in ipairs(espParts) do if a and a.Parent then a:Destroy() end end espParts={}
    end
end)

-- [5] Aimbot
states.aim = false
makeBtn("ðŸŽ¯ Aimbot",160,Color3.fromRGB(28,180,85),function(btn)
    states.aim = not states.aim
    btn.BackgroundColor3 = states.aim and Color3.fromRGB(21,230,140) or Color3.fromRGB(28,180,85)
    if states.aim then
        notify("Aimbot ON")
        if threads.aim then threads.aim:Disconnect() end
        threads.aim = game:GetService("RunService").Heartbeat:Connect(function()
            local chr = plr.Character
            if not states.aim or not chr or not chr:FindFirstChild("Head") then return end
            local minDist, target
            for _,other in ipairs(game:GetService("Players"):GetPlayers()) do
                if other~=plr and other.Character and other.Character:FindFirstChild("Head") and other.Character:FindFirstChild("Humanoid") then
                    if other.Character.Humanoid.Health>0 then
                        local dist = (other.Character.Head.Position-chr.Head.Position).Magnitude
                        if not minDist or dist<minDist then minDist,target=dist,other end
                    end
                end
            end
            if target then workspace.CurrentCamera.CFrame=CFrame.new(workspace.CurrentCamera.CFrame.Position,target.Character.Head.Position) end
        end)
    else
        notify("Aimbot OFF")
        if threads.aim then threads.aim:Disconnect() threads.aim=nil end
    end
end)

-- [6] Anti AFK
states.afk = false
makeBtn("ðŸ’¤ Anti AFK",202,Color3.fromRGB(93,174,145),function(btn)
    states.afk = not states.afk
    btn.BackgroundColor3 = states.afk and Color3.fromRGB(22,235,185) or Color3.fromRGB(93,174,145)
    if states.afk then
        notify("Anti AFK ligado")
        if threads.afk then threads.afk:Disconnect() end
        threads.afk = plr.Idled:Connect(function()
            pcall(function()
                game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                wait(1)
                game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            end)
        end)
    else
        notify("Anti AFK desligado")
        if threads.afk then threads.afk:Disconnect() threads.afk=nil end
    end
end)

-- [7] Chat Flood
states.flood = false
makeBtn("ðŸ’¬ Flood Chat",244,Color3.fromRGB(214,109,109),function(btn)
    states.flood = not states.flood
    btn.BackgroundColor3 = states.flood and Color3.fromRGB(240,40,45) or Color3.fromRGB(214,109,109)
    if states.flood then
        notify("Flood ON (use com cuidado/mute!)")
        if threads.flood then threads.flood:Disconnect() end
        threads.flood = game:GetService("RunService").Heartbeat:Connect(function()
            if not states.flood then return end
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("JL HUB NO TOPO ðŸš€","All")
            wait(1.25)
        end)
    else
        notify("Flood OFF")
        if threads.flood then threads.flood:Disconnect() threads.flood=nil end
    end
end)

-- [8] Auto Haki (busoshoku/observaÃ§Ã£o)
states.haki = false
makeBtn("ðŸ¥‹ Auto Haki (B/Obs)",288,Color3.fromRGB(100,118,210),function(btn)
    states.haki = not states.haki
    btn.BackgroundColor3 = states.haki and Color3.fromRGB(95,180,220) or Color3.fromRGB(100,118,210)
    if states.haki then
        notify("Auto Haki LIGADO")
        if threads.haki then threads.haki:Disconnect() end
        threads.haki = game:GetService("RunService").Heartbeat:Connect(function()
            local c = plr.Character
            if c then
                if c:FindFirstChild("Humanoid") and not c:FindFirstChild("HasBuso") then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                end
                if c:FindFirstChild("Humanoid") and not c:FindFirstChild("HasObservationHaki") then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Ken")
                end
            end
        end)
    else
        notify("Auto Haki desligado")
        if threads.haki then threads.haki:Disconnect(); threads.haki=nil end
    end
end)

